
import java.util.*;
/**
 * This class is part of the physical loc count test set
 */
@PhysicalLine(reason = "This is an annotation")
public class ComplexFile {
	
	/**
	 * Simple Javadoc
	 */
	protected static int inheritableVariable;  /* Block comment written in one line */
	private String fileName = "FirstClass.java"; // Keeps the file name for no good reason

	private static List<String> ordinaryList;

	/**
	 * This is a constructor
	 */
	public ComplexFile() {
		this.ordinaryList = new LinkedList<String>();
	}

	/**
	 * Useless method's javadoc
	 */
	private void doesNothing() {
		// Ordinary Comment

		System.out.printf("This file has %d lines", this.inheritableVariable);
		/* Formated output
		printed by printf */
	}

	@Annotation
	private class ComplexFileFirstLevelInnerClass {
	
		/**
		 * Simple Javadoc
		 */
		protected static int inheritableVariable;  /* Block comment written in one line */
		private String fileName = "FirstClass.java"; // Keeps the file name for no good reason
	
		private static List<String> ordinaryList;
	
		/**
		 * This is a constructor
		 */
		public ComplexFileFirstLevelInnerClass() {
			this.ordinaryList = new LinkedList<String>();
		}
	
		/**
		 * Useless method's javadoc
		 */
		private void doesNothing() {
			// Ordinary Comment
	
			System.out.printf("This file has %d lines", this.inheritableVariable);
			/* Formated output
			printed by printf */
		}

		@Annotation
		private class ComplexFileSecondLevelInnerClass {
				
			/**
			 * Simple Javadoc
			 */
			protected static int inheritableVariable;  /* Block comment written in one line */
			private String fileName = "FirstClass.java"; // Keeps the file name for no good reason
		
			private static List<String> ordinaryList;
		
			/**
			 * This is a constructor
			 */
			public ComplexFileSecondLevelInnerClass() {
				this.ordinaryList = new LinkedList<String>();
			}
		
			/**
			 * Useless method's javadoc
			 */
			private void doesNothing() {
				// Ordinary Comment
		
				System.out.printf("This file has %d lines", this.inheritableVariable);
				/* Formated output
				printed by printf */
			}
	
			@AnotherPhysicalLine	
			private enum SomeConstants {
				THIS, IS, AN, ENUMERATION
			}
	
			// No comment, javadoc, or blank lines are physical line
		}
		@AnotherPhysicalLine	
		private enum SomeConstants {
			THIS, IS, AN, ENUMERATION
		}
		
		// No comment, javadoc, or blank lines are physical line
	}
	
	@AnotherPhysicalLine	
	private enum SomeConstants {
		THIS, IS, AN, ENUMERATION
	}

	// No comment, javadoc, or blank lines are physical line
}


