
@AnAnnotationBeforeClass(Binding = "Some binding from an XML", unNamed = {
	"no name",
	"without name",
	"with no name",
	"with zero name",
	"devoid name",
	"unprovided name"
})
public class MultipleLinesAnnotations {
	
	@AnAnnotationWithParentheses(SomeSignificant = {
		"No Big Deal"
	})
	private int uselessVariable = 0;

	@AnAnnotationWithParameters(Binding =
		"It's a parameter"
	)
	private int evenMoreUselessVariable = -1;

	@AnotherAnnotation(SomeOrdeOfProcedures = {
		"first",
		"second",
		"third",
		"fourth",
		"fifth"
	})
	public OneLineAnnotation() {
		// Does Nothing
	}

	
	@AnnotationOfAnEnum
	protected enum InheritableConstants {
		ZERO, ONE, TWO, THREE,
		FOUR, FIVE, SIX, SEVEN,
		EIGHT, NINE, TEN
	};
			
	private void doesNothing() {
		System.out.println("I can't do anything");
	}

}
	
