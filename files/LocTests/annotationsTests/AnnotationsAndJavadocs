
@AnAnnotationBeforeClass(Binding = "Some binding from an XML", unNamed = {
	"Not Important"
})
public class NestedAnnotations {
	
	@AnAnnotationWithParentheses(
		type = "annotation"
	)
	private int uselessVariable = 0;

	@AnAnnotationWithParameters(
		iAm = "programmer"
	)
	private int evenMoreUselessVariable = -1;

	/**
	 * Javadoc
	 * @author Tercio de Melo
	 * @see AllTheTime
	 */
	@AnotherAnnotation(SomeOrdeOfProcedures = {
		"first",
		"second",
		"third",
		"fourth",
		"fifth"
	})
	public OneLineAnnotation() {
		// Does Nothing
	}

	
	@AnnotationOfAnEnum
	protected enum InheritableConstants {
		ZERO, ONE, TWO, THREE,
		FOUR, FIVE, SIX, SEVEN,
		EIGHT, NINE, TEN
	};
			
	private void doesNothing() {
		System.out.println("@IAmNotAnAnnotation");
	}

}
	
