#include<stdio.h>
main(){
int a;
int *aptr;
a=7;
aptr=&a;
printf("0 endere√ßo de a e %p\n"
			"O valor de aPtr e %p\n\n", &a, aptr);
printf("O valor de a e %d\n"
			"O valor de *aPtr e %d\n\n", a, *aptr);
printf("Sabendo que * e & complementam-se mutuamente."
			"\n&*aPtr = %p\n*&aPtr = %p\n",&*aptr,  *&aptr);
return 0;
}