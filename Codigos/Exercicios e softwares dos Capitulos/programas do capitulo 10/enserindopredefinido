#include<stdio.h>
#include<stdlib.h>

int main(void)
{
	struct tipo
	{
		int arq;
		struct tipo *tipo2;
	} abre, *abre2;

	abre.tipo2 = NULL;
	abre2=&abre;
	for (int nume = 1; nume <= 10; nume += 2)
	{
		abre2->tipo2 = (struct tipo *)malloc(sizeof(struct tipo));
		abre2 = abre2->tipo2;
		abre2->arq = nume;
		abre2->tipo2 = NULL;
	}
	abre2 = abre.tipo2;

	while (abre2)
	{
		printf("%d\n", abre2->arq);
		abre2 = abre2->tipo2;
	}
}