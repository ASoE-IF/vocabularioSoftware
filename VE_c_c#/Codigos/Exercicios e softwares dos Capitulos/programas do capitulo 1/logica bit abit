#include<stdio.h>

void main(void)
{
	printf("%d E %d = %d", 0, 0, 0 & 0);
	printf("\n%d E %d = %d", 1, 0, 1 & 0);
	printf("\n%d E %d = %d", 0, 1, 0 & 1);
	printf("\n%d E %d = %d", 1, 1, 1 & 1);
	printf("\n%d OU %d = %d", 0, 0, 0 | 0);
	printf("\n%d OU %d = %d", 1, 0, 1 | 0);
	printf("\n%d OU %d = %d", 0, 1, 0 | 1);
	printf("\n%d OU %d = %d", 1, 1, 1 | 1);
	printf("\n%d OU Ex. %d = %d", 0, 0, 0 ^ 0);
	printf("\n%d OU EX. %d = %d", 1, 0, 1 ^ 0);
	printf("\n%d OU EX. %d = %d", 0, 1, 0 ^ 1);
	printf("\n%d OU EX. %d = %d", 1, 1, 1 ^ 1);
}