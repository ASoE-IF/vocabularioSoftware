#include<stdio.h>

int main(void){

	static int campo[4][4]={1};
	int impr_linh, impr_col, remove1, remove2;
	for(impr_linh=0; impr_linh<4; impr_linh++){
				for(impr_col=0; impr_col<4; impr_col++){
				printf("[%d]", campo[impr_linh][impr_col]);
				remove1=impri_linh;
				remove2=impri_col;
				campo[impr_linh][impr_col]
				}
			printf("\n");
		}
	return 0;
}