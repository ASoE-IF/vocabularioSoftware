#include <stdlib.h>
#include <stdio.h>
unsigned long int fatorial (int n){
unsigned long int t, resposta; resposta = 1;
for (t = 1; t < n; t++)
resposta = resposta * t;
return(resposta);
}
void main(){
unsigned long f; int n;
printf("Digite um número: ");
scanf("%d", &n);
f = fatorial(n);
printf("O fatorial de %d é %ld\n", n, f);
}