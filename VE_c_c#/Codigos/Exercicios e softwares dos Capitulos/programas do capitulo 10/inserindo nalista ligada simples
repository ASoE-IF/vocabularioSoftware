#include<stdio.h>
#include<stdlib.h>

int main(void)
{
	int i;
	
	struct listaent
	{
		int numero;
		struct listaent *proximo;
	}inicio, *nodo, *anterior, *novo;
	
	inicio.proximo=NULL;	//Lista vazia.
	nodo=&inicio;				//Aponta para o inicio da lista.
	
	for(i=1; i<=10; i++)
		{
			nodo->proximo=(struct listaent *) malloc(sizeof(struct listaent));
			nodo=nodo->proximo;
			nodo->numero=i;
			nodo->proximo=NULL;
		}
		
		//exibe o inicio da lista.
		 nodo=inicio.proximo;
		 while(nodo)
		 {
		 	printf("%d", nodo->numero);
		 	nodo=nodo->proximo;
		 }
		 nodo=&inicio;
		 while(nodo->proximo){
			novo=malloc(sizeof(struct listaent));
			novo->proximo=nodo;
			anterior->proximo=novo;
			novo->numero=5;
		 }
}