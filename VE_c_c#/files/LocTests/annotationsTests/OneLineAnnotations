
@AnAnnotationBeforeClass
public class OneLineAnnotation {
	
	@AnAnnotationWithParentheses()
	private int uselessVariable = 0;

	@AnAnnotationWithParameters(Binding = "It's a parameter")
	private int evenMoreUselessVariable = -1;

	@AnotherAnnotation
	public OneLineAnnotation() {
		// Does Nothing
	}

	
	@AnnotationOfAnEnum
	protected enum InheritableConstants {
		ZERO, ONE, TWO, THREE,
		FOUR, FIVE, SIX, SEVEN,
		EIGHT, NINE, TEN
	};
			
	private void doesNothing() {
		System.out.println("I can't do anything");
	}

}
	
