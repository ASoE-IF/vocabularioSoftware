
import java.util.*;
@PhysicalLine(reason = "This is an annotation")
public class NoCommentsFile {
	
	protected static int inheritableVariable;  
	private String fileName = "FirstClass.java";

	private static List<String> ordinaryList;

	public NoCommentsFile() {
		this.ordinaryList = new LinkedList<String>();
	}

	private void doesNothing() {

		System.out.printf("This file has %d lines", this.inheritableVariable);
	}

	@Annotation
	private class NoCommentsFileFirstLevelInnerClass {
	
		protected static int inheritableVariable;  
		private String fileName = "FirstClass.java"; 
	
		private static List<String> ordinaryList;
	
		public NoCommentsFileFirstLevelInnerClass() {
			this.ordinaryList = new LinkedList<String>();
		}
	
		private void doesNothing() {
	
			System.out.printf("This file has %d lines", this.inheritableVariable);
		}

		@Annotation
		private class NoCommentsFileSecondLevelInnerClass {
				
			protected static int inheritableVariable;  
			private String fileName = "FirstClass.java"; 
		
			private static List<String> ordinaryList;
		
			public NoCommentsFileSecondLevelInnerClass() {
				this.ordinaryList = new LinkedList<String>();
			}
		
			private void doesNothing() {
		
				System.out.printf("This file has %d lines", this.inheritableVariable);
			}
	
			@AnotherPhysicalLine	
			private enum SomeConstants {
				THIS, IS, AN, ENUMERATION
			}
	
		}
		@AnotherPhysicalLine	
		private enum SomeConstants {
			THIS, IS, AN, ENUMERATION
		}
		
	}
	
	@AnotherPhysicalLine	
	private enum SomeConstants {
		THIS, IS, AN, ENUMERATION
	}

}


